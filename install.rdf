<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>clipple@mooz.github.com</em:id>
        <em:type>2</em:type> <!-- type of the addon, 2 means the extension -->
        <em:version>0.1.1</em:version>
        <em:name>Clipple</em:name>
        <em:creator>mooz</em:creator>
        <em:developer>mooz</em:developer>
        <em:homepageURL>http://wiki.github.com/mooz/clipple/</em:homepageURL>
        <em:optionsURL>chrome://clipple/content/preference.xul</em:optionsURL>
        <em:iconURL>chrome://clipple/skin/icon32/clipple.png</em:iconURL>
        <!-- en-US -->
        <em:description>Provides extended clipboard which allows you to store and paste the multiple text.</em:description>
        <!-- ja -->
        <em:localized>
            <Description>
                <em:locale>ja</em:locale>
                <em:name>Clipple</em:name>
                <em:creator>mooz</em:creator>
                <em:description>複数のテキストを保持し、張り付けることが可能となる拡張クリップボードを提供します。</em:description>
                <em:homepageURL>http://wiki.github.com/mooz/clipple/</em:homepageURL>
            </Description>
        </em:localized>
        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.0</em:minVersion>
                <em:maxVersion>7.0</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>

</RDF>
